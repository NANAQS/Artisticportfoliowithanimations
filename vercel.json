  "buildCommand": "prisma generate && prisma migrate deploy && next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false"
  },
  "crons": [
    {
      "path": "/api/init",
      "schedule": "0 0 * * *"
    }
  ]
}




